  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/vscode-webview": "^1.57.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
